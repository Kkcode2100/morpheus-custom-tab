{
  "code": "addon-url",
  "name": "Add-on URL",
  "plugin": "com.example.morpheus.SecSusPlugin",
  "version": "1.0.17",
  "minMorpheusVersion": "8.0.0",
  "settings": [
    { "name": "awsSecurityUrlTemplate",       "type": "string", "defaultValue": "https://{region}.console.aws.amazon.com/securityhub/home?region={region}" },
    { "name": "awsSustainabilityUrlTemplate", "type": "string", "defaultValue": "https://console.aws.amazon.com/billing/home#/carbon" },

    { "name": "azureSecurityUrlTemplate",     "type": "string", "defaultValue": "https://portal.azure.com/#view/Microsoft_Azure_Security/SecurityMenuBlade/~/overview" },
    { "name": "azureSustainabilityUrlTemplate","type": "string", "defaultValue": "https://portal.azure.com/#blade/Microsoft_Azure_Sustainability/EmissionsImpactDashboardBlade/Overview" },

    { "name": "gcpSecurityUrlTemplate",       "type": "string", "defaultValue": "https://console.cloud.google.com/security/command-center/findings?project={projectId}&organizationId={orgId}" },
    { "name": "gcpSustainabilityUrlTemplate", "type": "string", "defaultValue": "https://console.cloud.google.com/billing/{billingAccountId}/carbonfootprint?project={projectId}" }
  ]
}